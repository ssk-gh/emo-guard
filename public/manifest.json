{
  "name": "Web Keyword Blocker",
  "description": "Have a safe web journey.",
  "version": "1.0.0",
  "manifest_version": 3,
  "default_locale": "en",
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "activeTab",
    "alarms"
  ],
  "background": {
    "service_worker": "/static/js/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {}
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "/static/js/content-script.js"
      ],
      "run_at": "document_start"
    }
  ]
}