{
  "name": "Web Keyword Blocker",
  "description": "Have a safe web journey.",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "/static/js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {}
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/static/js/content_script.js"
      ]
    }
  ]
}